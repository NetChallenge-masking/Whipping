<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/image_navigation_1_4">

    <ImageView
        android:id="@+id/beacon_4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="230dp"
        android:layout_marginEnd="830dp"
        android:src="@drawable/ic_navigation_current"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/beacon_5"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="300dp"
        android:layout_marginEnd="450dp"
        android:src="@drawable/ic_navigation_current"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view4"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="330dp"
        android:background="#2196F3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view8"
        android:layout_width="50dp"
        android:layout_height="170dp"
        android:background="#2196F3"
        app:layout_constraintBottom_toBottomOf="@+id/view4"
        app:layout_constraintStart_toEndOf="@+id/view4"
        app:layout_constraintTop_toBottomOf="@+id/beacon_4" />

    <View
        android:id="@+id/view9"
        android:layout_width="315dp"
        android:layout_height="50dp"
        android:background="#2196F3"
        app:layout_constraintStart_toEndOf="@+id/view8"
        app:layout_constraintTop_toBottomOf="@+id/beacon_4" />

    <View
        android:id="@+id/view10"
        android:layout_width="50dp"
        android:layout_height="203dp"
        android:background="#2196F3"
        app:layout_constraintStart_toEndOf="@+id/view9"
        tools:layout_editor_absoluteY="157dp" />

    <View
        android:id="@+id/view11"
        android:layout_width="50dp"
        android:layout_height="160dp"
        android:background="#2196F3"
        app:layout_constraintBottom_toTopOf="@+id/view10"
        app:layout_constraintStart_toEndOf="@+id/view9" />

</androidx.constraintlayout.widget.ConstraintLayout>